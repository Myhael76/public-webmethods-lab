<?xml version="1.0" encoding="UTF-8"?>

<properties>

	<!-- Comments and resource bundle sections -->
	<comment>Monitor Behavior Configuration</comment>

	<entry key="defaultDays" type="String">
		<rendering-properties>
			<property name="type" value="choice"/>
			<property name="choices" value="all, work, day"/>
			<property name="label" value="Default Days"/>
			<property name="required" value="true"/>
		</rendering-properties>
			<value>day</value>
	</entry>

	<entry key="defaultMinutes" type="Short">
		<rendering-properties>
			<property name="type" value="textfield"/>
			<property name="size" value="25"/>
			<property name="hidden" value="true"/>
		</rendering-properties>
		<value>60</value>
	</entry>

    <entry key="maxKpiIntervalsToBackPopulate" type="Integer">
        <rendering-properties>
            <property name="type" value="textfield"/>
            <property name="size" value="25"/>
            <property name="hidden" value="true"/>
        </rendering-properties>
        <value>10</value>
    </entry>
</properties>
