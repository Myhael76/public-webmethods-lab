<?xml version="1.0" encoding="UTF-8"?>

<properties>

	<!-- Comments and resource bundle sections -->
	<comment>DataMaintenance Configuration Properties</comment>

	<entry key="businessDaysToRetain" type="Integer">
		<rendering-properties>
			<property name="type" value="textfield"/>
			<property name="size" value="25"/>
			<property name="label" value="Business Days To Retain"/>
			<property name="required" value="true"/>
		</rendering-properties>
		<value>90</value>
	</entry>

	<entry key="dataMaintenanceInterval" type="Long">
		<rendering-properties>
			<property name="type" value="textfield"/>
			<property name="size" value="25"/>
			<property name="label" value="Data Maintenance Interval"/>
			<property name="required" value="true"/>
		</rendering-properties>
		<value>4</value>
	</entry>

    <!-- List of aggregation intervals in minutes. List is a collection of strings -->
    <entry key="aggregationIntervalStrings" type="List">
        <rendering-properties>
            <property name="type" value="list"/>
            <property name="rows" value="5"/>
            <property name="column.size" value="25"/>
            <property name="hidden" value="true"/>
        </rendering-properties>
        <!-- 1 hour -->
        <list-value>60</list-value>
    </entry>

    <entry key="aggregationDaysToRetain" type="Integer">
        <rendering-properties>
            <property name="type" value="textfield"/>
            <property name="size" value="25"/>
            <property name="label" value="Aggregated Business Days To Retain"/>
						<property name="required" value="true"/>
        </rendering-properties>
        <value>365</value>
    </entry>
</properties>
